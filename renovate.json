{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",

  "extends": [
    "config:base",
    ":rebaseStalePrs",
    ":prHourlyLimit4",
    ":labels(dependencies)"
  ],

  "commitMessageExtra": "from {{{currentValue}}} to {{{newValue}}}",

  "hostRules": [
    {
      "matchHost": "registry.npmjs.org",
      "encrypted": {
        "token": ""
      }
    }
  ]
}
